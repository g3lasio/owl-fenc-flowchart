{
  "name": "owl-fence-estimator-backend",
  "version": "1.0.0",
  "description": "Backend para Owl Fence Estimator con motores DeepSearch para generaci√≥n de estimados",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "ts-node -r tsconfig-paths/register src/index.ts",
    "build": "tsc",
    "watch": "tsc -w",
    "test": "mocha -r ts-node/register tests/**/*.ts",
    "test:unit": "mocha -r ts-node/register tests/unit/**/*.ts",
    "test:jest": "jest",
    "test:materials": "ts-node -r tsconfig-paths/register src/test-material-engine.ts",
    "start:deepsearch": "ts-node -r tsconfig-paths/register src/microservice-runner.ts deepsearch-engine",
    "start:unstructured": "ts-node -r tsconfig-paths/register src/microservice-runner.ts unstructured-input",
    "start:error-handler": "ts-node -r tsconfig-paths/register src/microservice-runner.ts error-handler",
    "start:microservices": "concurrently \"npm run start:deepsearch\" \"npm run start:unstructured\" \"npm run start:error-handler\"",
    "docker:build": "cd .. && ./deploy.sh build",
    "docker:start": "cd .. && ./deploy.sh start",
    "docker:stop": "cd .. && ./deploy.sh stop"
  },
  "keywords": [
    "estimate",
    "fencing",
    "ai",
    "deepsearch"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.40.0",
    "@supabase/supabase-js": "^2.49.4",
    "@types/express": "^5.0.1",
    "@types/node": "^22.15.3",
    "bullmq": "^5.51.1",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express-rate-limit": "^7.5.0",
    "helmet": "^8.1.0",
    "html-pdf-node": "^1.0.8",
    "node-cache": "^5.1.2",
    "openai": "^4.96.0",
    "pdfkit": "^0.17.0",
    "puppeteer": "^24.7.2",
    "redis": "^4.7.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/chai": "^5.2.1",
    "@types/cors": "^2.8.17",
    "@types/jest": "^29.5.14",
    "@types/mocha": "^10.0.10",
    "@types/pdf-parse": "^1.1.5",
    "@types/sinon": "^17.0.4",
    "@types/uuid": "^10.0.0",
    "axios": "^1.9.0",
    "chai": "^5.2.0",
    "ejs": "^3.1.10",
    "express": "^5.1.0",
    "jest": "^29.7.0",
    "mocha": "^11.2.2",
    "multer": "^1.4.5-lts.2",
    "sinon": "^20.0.0",
    "ts-jest": "^29.3.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3"
  }
}
